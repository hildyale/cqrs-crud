# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           application.controllers.HomeController.index
GET     /vehiculos                  application.controllers.VehiculosViewController.getVehiculos
POST     /vehiculos                 application.controllers.VehiculosController.addVehiculo
PUT     /vehiculos                  application.controllers.VehiculosController.updateVehiculo
DELETE     /vehiculos               application.controllers.VehiculosController.deleteVehiculo

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
